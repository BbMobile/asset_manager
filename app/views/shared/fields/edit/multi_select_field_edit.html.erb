<td><%= field_name %></td>
<% if value != nil %>
    <td><%=  select(field_name,field_name, Field.find(field_id).field_option.collect {|p| [ p.option, p.id ] },{:selected  => value.field_option_id[0]},{:multiple => true}) %></td>
<% else %>
    <td><%=  select(field_name,field_name, Field.find(field_id).field_option.collect {|p| [ p.option, p.id ] },{},{:multiple => true}) %></td>
<% end %>
<%= hidden_field(field_name, field_name+'_type',:value=> 'multi_option') %>