<%= render :template => 'shared/navbar' %>

<%= render :template => 'search/sidebar'  %>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Actions</th>
  </tr>
  <% @assets.each do |asset| %>
      <tr>
        <td>
          <%= asset.name %>
        </td>

        <td>
          <%= asset.description %>
        </td>
      </tr>
  <% end %>
</table>